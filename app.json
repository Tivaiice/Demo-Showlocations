    "ios": {
      "bundleIdentifier": "com.pany.demoios",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.905775505311-45b2793ilm3k22kqoi188uq44of1l8mv"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.pany.demoandroid",
      "googleServicesFile": "./google-services.json"
    }
  }
}
